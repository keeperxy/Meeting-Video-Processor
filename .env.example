# Environment Variables for Meeting Video Processor
# Copy this file to .env and fill in your actual values

# Google Gemini API Configuration
GEMINI_API_KEY=

# System Tools Paths
FFMPEG_PATH=ffmpeg
HANDBRAKE_PATH=HandBrakeCLI

# HandBrake Configuration
HANDBRAKE_PRESET_FILE=Meeting.json
HANDBRAKE_PRESET_NAME=Meeting

# Processing Configuration
FRAME_INTERVAL=60
DEFAULT_PROMPT=prompt.txt

# Gemini Model Configuration
GEMINI_MODEL=gemini-2.5-pro

# Runtime Flags
DEBUG=false
DRY_RUN=false
NO_CLEANUP=false
MODEL_LIMITS_FILE=model_limits.json
