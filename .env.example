# Meeting Processor Environment Configuration
# Copy this file to .env and adjust the values as needed

# FFmpeg and HandBrake paths
FFMPEG_PATH=ffmpeg
HANDBRAKE_PATH=HandBrakeCLI
HANDBRAKE_PRESET_FILE=Meeting.json
HANDBRAKE_PRESET_NAME=Meeting

# Processing settings
FRAME_INTERVAL=60
DEFAULT_PROMPT=prompt.txt

# Date source preference
# Options: "metadata" (default), "file_mtime", or "manual"
# - metadata: Prefer creation time from video metadata, fallback to file modification time (no confirmation)
# - file_mtime: Prefer file modification time, fallback to metadata (no confirmation)
# - manual: Always prompt for manual date/time input
PREFERRED_DATE_SOURCE=metadata

# Google Gemini settings
GEMINI_MODEL=gemini-2.5-pro
GEMINI_API_KEY=your_gemini_api_key_here

# Model limits file
MODEL_LIMITS_FILE=model_limits.json

# Debug and control flags
DEBUG=false
DRY_RUN=false
NO_CLEANUP=false 